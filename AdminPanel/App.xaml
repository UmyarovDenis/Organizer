<Application x:Class="AdminPanel.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AdminPanel"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="pack://application:,,,/OrganizerStyles;component/Common/Colors.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/OrganizerStyles;component/Common/Images.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/OrganizerStyles;component/Common/WindowButtons.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/OrganizerStyles;component/Common/Lists.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/OrganizerStyles;component/Common/Borders.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/OrganizerStyles;component/Server/Buttons.xaml"/>
                <ResourceDictionary Source="pack://application:,,,/OrganizerStyles;component/Server/TrayMenu.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
